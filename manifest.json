  "manifest_version": 2,
  "omnibox": {"keyword": "b64"},
  "icons": {
    "16": "global_international_internet_browser.png",
    "48": "global_international_internet_browser_48.png",
    "128": "global_international_internet_browser_128.png"
  },
  "background": {
	"scripts": ["background.js"],
	"persistent": false
  }
}
